{
  "name": "gotrue-js-examples",
  "version": "1.0.0",
  "description": "A playground for Netlify's gotrue-js library",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently 'yarn start:client' 'yarn start:server'",
    "start:client": "webpack-dev-server --hot --config webpack.config.js",
    "start:server": "netlify-lambda serve src/lambda",
    "build": "yarn build:client && yarn build:server",
    "build:client": "webpack --config webpack.config.js",
    "build:server": "netlify-lambda build src/lambda"
  },
  "author": "luna",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^3.5.1",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "file-loader": "^1.1.11",
    "gotrue-js": "^0.9.20",
    "netlify-lambda": "^0.4.0",
    "node-fetch": "^2.1.2",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2"
  }
}
